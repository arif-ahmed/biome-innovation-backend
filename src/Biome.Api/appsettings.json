{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "super-secret-key-that-should-be-in-env-vars-at-least-32-chars",
    "ExpiryMinutes": 60,
    "Issuer": "BiomeInnovation",
    "Audience": "BiomeInnovation"
  },
  "Persistence": {
    "Provider": "InMemory"
  },
  "AWS": {
    "Profile": "default",
    "Region": "us-east-1",
    "ServiceURL": "http://localhost:8000"
  }
}