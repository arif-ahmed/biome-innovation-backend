{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "super-secret-key-that-should-be-in-env-vars-at-least-32-chars",
    "ExpiryMinutes": 60,
    "Issuer": "BiomeInnovation",
    "Audience": "BiomeInnovation"
  },
  "Persistence": {
    "Provider": "DynamoDb",
    "LocalStack": {
      "Enabled": false,
      "EnsureTablesCreated": false,
      "ServiceUrl": "http://localhost:4566",
      "Region": "us-east-1"
    }
  },
  "AWS": {
    "Profile": "default",
    "Region": "us-east-1"
  }
}
